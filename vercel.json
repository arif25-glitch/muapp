{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "rewrites": [
                {
                    "source": "/(.*)",
                    "destination": "/api"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}